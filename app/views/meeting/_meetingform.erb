<div class="newform">
      <form action="/meeting/new" method="POST">
        <label>Title:</label>
        <input type="text" name="meeting[title]" placeholder="Meeting Title"><br>
        <label>Description:</label>
        <textarea type="text" name="meeting[description]" placeholder="Meeting Description"></textarea><br>
        <label>Participants:</label>
        <input type="text" name="participant" placeholder="Enter participants"><br><br>
        <input class="button" type="submit" value="Start Meeting">
      </form>
</div>
